generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id String @id @default(uuid())
    name String
    email String
    isAdmin Boolean
    preferences Json?
    posts Post[]
}


model Post {
    rating Float
    createdAt DateTime
    updatedAt DateTime
    author User @relation(fields: [userId], references:[id])
    userId String
}